var checkIP = (rule, value, cb) => {
             const regIP =  /^(\d{1,2}|1\d\d|2[0-4]\d|25[0-5])\.(\d{1,2}|1\d\d|2[0-4]\d|25[0-5])\.(\d{1,2}|1\d\d|2[0-4]\d|25[0-5])\.(\d{1,2}|1\d\d|2[0-4]\d|25[0-5])$/
             if (regIP.test(value)) {
               return cb()
             }
             cb(new Error('请输入正确的IP格式'))
           }
